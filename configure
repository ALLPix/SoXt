for ac_dir in $ac_dummy; do
  IFS=$ac_save_IFS
  test -z "$ac_dir" && ac_dir=.
  if test -f "$ac_dir/$ac_word"; then
   ac_cv_path_sim_ac_doxygen_exe=$ac_dir/$ac_word
   break
fi
done

  test -z "$ac_cv_path_sim_ac_doxygen_exe" && ac_cv_path_sim_ac_doxygen_exe="false"
  ;;
esac
fi
sim_ac_doxygen_exe=$ac_cv_path_sim_ac_doxygen_exe
if test -n "$sim_ac_doxygen_exe"; then
  echo "configure:6602: result: $sim_ac_doxygen_exe" >&5
echo "${ECHO_T}$sim_ac_doxygen_exe" >&6
else
  echo "configure:6605: result: no" >&5
echo "${ECHO_T}no" >&6
fi

  if test x"$sim_ac_doxygen_exe" = xfalse; then
    { echo "configure: error: Could not find the doxygen tool -- see http://www.stack.nl/~dimitri/doxygen/" >&2; exit 1; }
  else
    sim_ac_doxygen_avail=`$sim_ac_doxygen_exe -help 2> /dev/null | head -1 | sed 's%[^ ]\+ [^ ]\+ %%'`

  fi
fi

  sim_ac_doxygen_our_version=1.1.5
  if test x"$sim_ac_doxygen_avail" != x"$sim_ac_doxygen_our_version"; then
    echo "configure: WARNING: You are not using Doxygen version $sim_ac_doxygen_our_version" >&2
  fi

  # Extract the first word of "perl", so it can be a program name with args.
set dummy perl; ac_word=$2
echo "configure:6624: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6
if test "${ac_cv_path_sim_ac_perl_exe+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  case $sim_ac_perl_exe in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_sim_ac_perl_exe="$sim_ac_perl_exe" # Let the user override the test with a path.
  ;;
  *)
  ac_save_IFS=$IFS; IFS=':'
ac_dummy="$PATH"
for ac_dir in $ac_dummy; do
  IFS=$ac_save_IFS
  test -z "$ac_dir" && ac_dir=.
  if test -f "$ac_dir/$ac_word"; then
   ac_cv_path_sim_ac_perl_exe=$ac_dir/$ac_word
   break
fi
done

  test -z "$ac_cv_path_sim_ac_perl_exe" && ac_cv_path_sim_ac_perl_exe="false"
  ;;
esac
fi
sim_ac_perl_exe=$ac_cv_path_sim_ac_perl_exe
if test -n "$sim_ac_perl_exe"; then
  echo "configure:6651: result: $sim_ac_perl_exe" >&5
echo "${ECHO_T}$sim_ac_perl_exe" >&6
else
  echo "configure:6654: result: no" >&5
echo "${ECHO_T}no" >&6
fi

  if test x"$sim_ac_perl_exe" = xfalse; then
    echo "configure: WARNING: Could not find the Perl executable" >&2
  fi

fi

